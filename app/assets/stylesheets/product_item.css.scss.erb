body {
  overflow-x: hidden;  
  overflow-y: hidden; 
}

/*弹出图层*/
.tc_wrap{
  width:960px;
  margin:0 auto;  
  background:#fff;
  font-size:14px;
  padding-bottom:20px;
}
.tc_top{
  width:926px;
  height:135px;
  border-top:4px solid #f8277b;
  border-bottom:8px solid #636363;
  position:relative;
  padding:10px 0 0 34px;
}
.tc_top .tc_text{
  height:32px;
  line-height:32px;
  color:#000;
  font-size:14px;
  position:relative;
}
.tc_top .tc_text .p_ul{
  position:absolute;
  top:10px;
  left:105px;
  width:130px;
  height:13px;
}
.tc_top .tc_text .p_ul li{
  width:15px;
  float:left;
  margin-right:4px;
}
.tc_top .tc_text strong{
  font-size:24px;
}
.tc_top .tc_ico{
  width:100px;
  height:36px;
  line-height:36px;
  text-align:center;
  background:url(/assets/ico12.jpg) no-repeat;
}
.tc_top .tc_ico a{
  color:#fff;
  font-size:14px;
}
.tc_top  .tc_ico2{
  position:absolute;
  top:20px;
  right:0;
  width:142px;
  background:url(/assets/ico8.jpg) no-repeat left 3px;
}
.tc_top  .tc_ico2 p{
  line-height:24px;
  text-indent:36px;
}
.tc_top  .tc_ico2 strong{
  color:#ff6699;
  font-size:16px;
}
.tc_top  .tc_ico3{
  position:absolute;
  top:83px;
  right:122px;
  background:url(/assets/ico9.jpg) no-repeat;
  width:65px;
  height:26px;
  line-height:26px;
  text-indent:26px;
  font-weight:bold;
}
.tc_top  .tc_ico4{
  position:absolute;
  top:83px;
  right:40px;
  background:url(/assets/ico10.jpg) no-repeat;
  width:65px;
  height:26px;
  line-height:26px;
  text-indent:26px;
  font-weight:bold;
}
.tc_top  .tc_ico3 a,.tc_top  .tc_ico4 a{
  font-size:14px;
  color:#fff;
}
.tc_sum{
  width:894px;
  margin:0 auto;
  padding-top:10px;
}
.tc_sum .pic{
  width:210px;
  height:210px;
  border:1px solid #c6c8c7;
  overflow:hidden;
  float:left;
}
.tc_sum .pic_r{
  width:640px;
  float:right;
  border-top:1px dashed #dbdbdb;
}
.tc_sum .pic_r p{
  line-height:18px;
  padding:5px 0 0 0;
  color:#000;
}
.tc_sum .pic_r .ul{
  border-top:1px dashed #dbdbdb;
  padding-top:20px;
}
.tc_sum .pic_r .ul li{
  width:80px;
  height:80px;
  padding:4px;
  border:1px solid #efefef;
  margin-right:5px;
  overflow:hidden;
  float:left;
}
.tc_sum .pic_r .text{

}
.tc_sum .pic_r .text a{
  padding:2px;
  background:#ff306c;
  margin-right:15px;
  color:#fff;
}
.tc_sum2{
  width:894px;
  height:80px;
  margin:0 auto;
  padding-top:40px;
}
.tc_sum2 dt{
  width:48px;
  height:48px;
  overflow:hidden;
  float:left;
}
.tc_sum2 dd{
  width:820px;
  float:right;  
}
.tc_sum2 p{
  line-height:18px;
}
.tc_sum2 p.p2{
  font-size:12px;
  color:#c8c8c9;
}
.tc_sum2 p.p2 a{
  color:#767676;
}